<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Positive_InitialCost</fullName>
    <active>true</active>
    <errorConditionFormula>InitialCost__c &lt;= 0</errorConditionFormula>
    <errorDisplayField>InitialCost__c</errorDisplayField>
    <errorMessage>Initial cost must be positive</errorMessage>
</ValidationRule>
